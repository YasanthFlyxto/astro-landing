---
interface Props {}

const {} = Astro.props;
---

<div class="mx-auto max-w-7xl px-6 lg:px-8">
  <p class="text-center mb-12 text-xl">Provided Services For</p>
  <div
    class="mx-auto grid max-w-lg grid-cols-6 items-center gap-x-8 gap-y-12 sm:max-w-xl sm:grid-cols-6 sm:gap-x-10 sm:gap-y-14 lg:mx-0 lg:max-w-none lg:grid-cols-5"
  >
    <img
      class="col-span-2 sm:block hidden max-h-12 w-full object-contain lg:col-span-1"
      src="./brands/metaxx.png"
      alt="Tuple"
      width="158"
      height="48"
    />
    <img
      class="col-span-2 max-h-16 w-full object-contain lg:col-span-1"
      src="./brands/lever.webp"
      alt="Transistor"
      width="158"
      height="48"
    />
    <img
      class="col-span-2 max-h-16 w-full object-contain lg:col-span-1"
      src="./brands/emblem.png"
      alt="Reform"
      width="158"
      height="48"
    />

    <img
      class="col-span-2 max-h-16 w-full object-contain sm:col-start-2 lg:col-span-1"
      src="./brands/lions.png"
      alt="SavvyCal"
      width="158"
      height="48"
    />
    <img
      class="col-span-2 col-start-2 max-h-12 w-full object-contain sm:col-start-auto lg:col-span-1"
      src="./brands/forge.png"
      alt="Statamic"
      width="158"
      height="48"
    />
    <img
      class="col-span-2 max-h-16 sm:hidden block w-full object-contain lg:col-span-1"
      src="./brands/metaxx.png"
      alt="Tuple"
      width="158"
      height="48"
    />
  </div>
  <div class="flex flex-col mt-24 sm:mt-24  items-center  ">
    <p class="sm:text-3xl text-xl text-blue-900 text-center">You Can Be A Great Client Too.</p>
    <div class="flex justify-center mt-4 sm:mt-12">
      <p
        class="relative rounded-full transition px-8 py-3 text-xl leading-6 text-blue-900 ring-1 ring-inset ring-blue-900 hover:ring-blue-500"
      >
        <a href="/contact" class="font-semibold">
          <span class="absolute inset-0" aria-hidden="true"></span>
          Contact Us <span aria-hidden="true">&rarr;</span>
        </a>
      </p>
    </div>
  </div>
</div>
